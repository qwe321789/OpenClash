  function createFixedBottomBannerWithClose(link) {
    const banner = document.createElement('div');
    banner.style.cssText = `position:fixed;bottom:0;font-size:20px;left:0;width:100%;background-color:rgba(0,0,0,.7);color:white;text-align:center;padding:15px;z-index:9999;display:flex;justify-content:center;align-items:center;`;

    const message = document.createElement('span');
    message.textContent = '防失联,请点击这里安装APP';
    message.style.cssText = 'cursor:pointer;margin-right:10px;';
    message.onclick = (event) => { window.location.href = link; event.stopPropagation(); };

    const closeButton = document.createElement('span');
    closeButton.textContent = '×';
    closeButton.style.cssText = 'cursor:pointer;font-size:20px;font-weight:bold;margin-left:10px;';
    closeButton.onclick = (event) => { banner.remove(); event.stopPropagation(); };

    banner.append(message, closeButton);
    document.body.append(banner);
  }
